@model Passion_Project.Models.ReviewDto

@{
    ViewBag.Title = "DeleteConfirm";
}

<h2>Are you sure you want to delete this review?</h2>
<a href="/Review/List">Cancel</a>
<h2>@Model.Rating by @Model.UserName</h2>
<form action="/Review/Delete/@Model.ReviewID" method="POST">
    <input type="submit" value="Confirm Delete" />

</form>